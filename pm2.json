{
  "apps": [
    {
      "name": "proxy",
      "script": "proxy.js"
    },
    {
      "name": "web",
      "script": "node_modules/.bin/webpack-dev-server",
      "args": [
        "src/_entry.js",
        "--inline",
        "--content-base", "www",
        "--port", "8083"
      ],
      "cwd": "yunity-webapp"
    },
    {
      "name": "mobile",
      "script": "node_modules/.bin/webpack-dev-server",
      "args": [
        "--port", "8084"
      ],
      "cwd": "yunity-webapp-mobile"
    },
    {
      "name": "crossbar",
      "script": "env/bin/crossbar",
      "args": [
        "start"
      ],
      "exec_interpreter": "env/bin/python",
      "exec_mode": "fork_mode",
      "treekill": false,
      "cwd": "yunity-core"
    },
    {
      "name": "django",
      "script": "manage.py",
      "args": [
        "runserver",
        "--noreload",
        "8000"
      ],
      "exec_interpreter": "env/bin/python",
      "exec_mode": "fork_mode",
      "cwd": "yunity-core"
    }
  ]
}
