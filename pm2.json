{
  "apps": [
    {
      "name": "proxy",
      "script": "proxy.js"
    },
    {
      "name": "web",
      "script": "node_modules/.bin/webpack-dev-server",
      "args": [
        "src/_entry.js",
        "--inline",
        "--content-base", "www",
        "--port", "8083"
      ],
      "cwd": "yunity-webapp"
    },
    {
      "name": "mobile",
      "script": "node_modules/.bin/webpack-dev-server",
      "args": [
        "--port", "8084"
      ],
      "cwd": "yunity-webapp-mobile"
    },
    {
      "name": "sockets",
      "script": "index.js",
      "args": [
        "-w", "8080",
        "-p", "9080"
      ],
      "cwd": "yunity-sockets"
    },
    {
      "name": "django",
      "script": "manage.py",
      "args": [
        "runserver",
        "--noreload",
        "8000"
      ],
      "exec_interpreter": "env/bin/python",
      "exec_mode": "fork_mode",
      "cwd": "yunity-core"
    }
  ]
}
